import { Xslt, XmlParser } from 'xslt-processor';
import { getXSLTSchemas } from '../showMxTemplateAsTreeView/showTempAction';
const xslt = new Xslt();
const xmlParser = new XmlParser();

function Code () {
useEffect(() => {
    try {
        getXSLTSchemas().then(resSchemas => {              
            // Get a JSON object from each of the responses
           const schemaPromises= resSchemas.map(result => {
                if(result.status === 'fulfilled') return result.value.text();
                else return Promise.reject(new Error(`XSD Schema issue : ${result.reason}`))
           });
           return Promise.allSettled(schemaPromises);
        })
        .then(resData => {
            const schemaData = resData.map(result => {
                if(result.status === 'fulfilled') return result.value;
                else return null;
            })     
             
            console.log('schemaData', schemaData)   
            xslt.xsltProcess(
                xmlParser.xmlParse(schemaData[0]),
                xmlParser.xmlParse(schemaData[1])
            )
            .then((result) => {
                console.log('output', result)
            })
            .catch(error => {
                console.log('error', error);
            })
        })     
    }
    catch(error)  {
        console.log('error', error);
    }       
    }, [])
}
